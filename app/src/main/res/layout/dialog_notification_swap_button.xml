<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:paddingBottom="40dp"
        android:background="@drawable/customborder"
        android:id="@+id/relativeLayout9">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Are you sure you want reject Thuyet&apos;s swap request"
            android:id="@+id/textView35"
            android:layout_alignParentTop="false"
            android:textColor="#ffffff"
            android:layout_alignParentRight="false"
            android:layout_alignParentEnd="false"
            android:gravity="center"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp" />

        <Button
            android:layout_width="181dp"
            android:layout_height="36dp"
            android:text="Yes"
            android:id="@+id/btn_dialog_notification_swap"
            android:background="@drawable/bg_white_popup"
            android:textColor="#f76565"
            android:layout_below="@+id/textView35"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="60dp"
            android:layout_marginTop="20dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imv_clode_dialog_not_like"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:src="@drawable/btn_close_filter" />
    </RelativeLayout>

</RelativeLayout>